	<queryString>
		<![CDATA[-- Pega aquí la consulta SQL
        SELECT
            o.id AS id_organizacion,
            objetivo->>'nombreObjetivo' AS nombre_objetivo,
            meta->>'nombreMeta' AS nombre_meta,
            indicador->>'nombreIndicador' AS nombre_indicador,
            indicador->>'tipo' AS tipo,
            indicador->>'formula' AS formula,
            indicador->>'nivelAlerta1' AS nivel_alerta_1,
            indicador->>'nivelAlerta2' AS nivel_alerta_2,
            indicador->>'nivelAlerta3' AS nivel_alerta_3,
            indicador->>'periodicidad' AS periodicidad
        FROM proveedores.organizaciones o
        CROSS JOIN LATERAL jsonb_array_elements(o.objetivos_indicadores->'objetivos') AS objetivo
        LEFT JOIN LATERAL jsonb_array_elements(COALESCE(objetivo->'metas', '[]'::jsonb)) AS meta ON true
        LEFT JOIN LATERAL jsonb_array_elements(COALESCE(meta->'indicadores', '[]'::jsonb)) AS indicador ON true
        WHERE  o.id = $P{id}
        ORDER BY  o.id, nombre_objetivo, nombre_meta, nombre_indicador]]>
